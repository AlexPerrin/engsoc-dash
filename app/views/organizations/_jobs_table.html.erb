<% if @organization.jobs.any? %>
  <div class="table-responsive mh">
    <table class="table table-hover">
      <tbody>
        <%= render @jobs %>
      </tbody>
    </table>
  </div>
<% else %>
  <div class="alert alert-warning mv mh">
    There are currently no jobs in this organization.
  </div>
<% end %>
