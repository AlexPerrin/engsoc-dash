<div class="table-responsive mh">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Title</th>
        <th>Deadline</th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
        <% if job.job_posting.present? && job.job_posting.status == "open" %>
          <%= render "active_job_postings", :job_posting => job.job_posting %>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
