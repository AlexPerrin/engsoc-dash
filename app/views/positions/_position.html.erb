<tr>
  <td class="col-md-2">
    <%= link_to position.user.first_name + " " + position.user.last_name, profile_path(position.user.id) %>
  </td>
  <td class="col-md-2">
    <%= position.user.email %>
  </td>
  <td class="col-md-2">
    <% if position.active? %>
      Active
    <% end %>
  </td>
  <td class="col-md-2">
    <%= position.start_date.to_date %>
  </td>
  <td class="col-md-2">
    <%= position.end_date.to_date %>
  </td>
</tr>